{
  "name": "smartkidzclub.app",
  "version": "2.0.0",
  "private": true,
  "main": "firebase.function.js",
  "engines": {
    "node": "12"
  },
  "scripts": {
    "env": "node .env.js",
    "dev": "cross-env NODE_ENV=test yarn env && firebase use stage && next dev -p 5500",
    "build": "next build",
    "start": "next start",
    "logs": "firebase functions:log",
    "shell": "yarn build && firebase functions:shell",
    "serve": "yarn build && firebase emulators:start --only functions,hosting",
    "deploy": "cross-env NODE_ENV=test yarn env && yarn build && firebase use stage && firebase deploy --only functions:skc_app,hosting",
    "deploy:prod": "cross-env NODE_ENV=production yarn env && yarn build && firebase use prod && firebase deploy --only functions:skc_app,hosting"
  },
  "dependencies": {
    "@netlify/plugin-nextjs": "^3.0.3",
    "@stripe/react-stripe-js": "^1.4.0",
    "@stripe/stripe-js": "^1.13.1",
    "canvas-confetti": "^1.4.0",
    "clsx": "^1.1.1",
    "firebase": "^8.3.0",
    "firebase-admin": "^9.5.0",
    "firebase-functions": "^3.13.2",
    "is-email": "^1.0.0",
    "next": "10.0.9",
    "next-firebase-auth": "^0.12.2",
    "nprogress": "1.0.0-1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-firebaseui": "^4.1.0",
    "react-modal": "^3.12.1",
    "react-simply-carousel": "^4.2.0",
    "react-toast": "^1.0.1",
    "stripe": "^8.138.0",
    "styled-components": "^5.2.1",
    "superstruct": "^0.14.2"
  },
  "devDependencies": {
    "@types/canvas-confetti": "^1.3.0",
    "@types/node": "14.14.35",
    "@types/nprogress": "0.2.0",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.2",
    "@types/react-modal": "^3.12.0",
    "@types/styled-components": "5.1.9",
    "babel-plugin-styled-components": "^1.12.0",
    "cross-env": "^7.0.3",
    "typescript": "4.2.3"
  }
}
